<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.scu.edu.popartistdirectory" >
    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:logo="@drawable/appicon"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ConcertInformation" android:label="Concert Information"/>
        <activity android:name=".FavoriteArtists" android:label="Favorite Artists"/>
        <activity android:name=".GetArtistDetails" android:label="Artists Information"/>


    </application>

</manifest>
